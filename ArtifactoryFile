pipeline {
  agent any
  stages {
      stage('Clone') {
        steps {
          git branch: 'master', url: "https://github.com/mulpuruvs/simple-java-maven-app.git"
        }
      }
      
      stage('Artifactory configuration') {
        steps {
        
            rtServer {
              id: "ARTIFACTORY_SERVER",
              url: "http://54.244.9.91:8081/artifactory",
              credentialsId: "ArtifactoryCredentials"
            }
          
        }
      }
      
      
      
  }
}  
